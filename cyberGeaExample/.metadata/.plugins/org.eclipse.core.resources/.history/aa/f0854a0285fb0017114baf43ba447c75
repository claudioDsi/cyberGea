package org.cybergea.dbmanager.provider;

public class Environment {
	
	private int id;
	private Integer humidity;
	private Float temperature;
	private Integer light;
	private String season;
	private Float air;
	
	public Environment() {
		
	}
	
	public Environment(int id,Integer humidity,Float temperature,Integer light,String season,Float air) {
		this.id=id;
		this.humidity=humidity;
		this.temperature=temperature;
		this.light=light;
		this.season=season;
		this.air=air;
	}

	@Override
	public String toString() {
		return "Environment [id=" + id + ", humidity=" + humidity + ", temperature=" + temperature + ", light=" + light
				+ ", season=" + season + ", air=" + air + "]";
	}

	public int getId() {
		return id;
	}

	public Integer getHumidity() {
		return humidity;
	}

	public Float getTemperature() {
		return temperature;
	}

	public Integer getLight() {
		return light;
	}

	public String getSeason() {
		return season;
	}

	public Float getAir() {
		return air;
	}

	public void setId(int id) {
		this.id = id;
	}

	public void setHumidity(Integer humidity) {
		this.humidity = humidity;
	}

	public void setTemperature(Float temperature) {
		System.out.println("sonoinSet");
		this.temperature = temperature;
	}

	public void setLight(Integer light) {
		this.light = light;
	}

	public void setSeason(String season) {
		this.season = season;
	}

	public void setAir(Float air) {
		this.air = air;
	}
		
}
