package org.cyberGea.eventGenerators;

public class PressureSensor extends Sensor{
	
	int pressure;
	Thread t;

	public PressureSensor(DataPublisher clientReference, double min, double max, String unitMeasurement,
			String topicName,int pressure) {
		super(clientReference, min, max, "kg", "rawData/soilPressure");
		this.pressure=pressure;
		
		
	}
	
	public void startMeasurePressure() {
		System.out.println("the kg of soil is");
		t.start();
	}

	

}
