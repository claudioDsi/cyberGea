package org.cyberGea.eventGenerators;

public class HumiditySensor extends Sensor{
	int humidity;
	 String name;
	 Thread t;
	
	public HumiditySensor(DataPublisher clientReference, int min, int max, String name) {
		super(clientReference, min, max, "%", "rawData/humidity", name);
		this.name=name;
		startHumiditySensor();
		
		
	}
	
	public void startHumiditySensor() {
		System.out.println(name + " is started");
		t=new Thread(this);
		t.start();
	}
	public void stopHumiditySensor() {
		this.stopThread();
		System.out.println(name+ " is stopped");
	}

}
