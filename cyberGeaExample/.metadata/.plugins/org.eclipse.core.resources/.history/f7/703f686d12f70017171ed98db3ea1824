package org.cybergea.dbmanager.provider;

public class Soil {
	private int id_soil;
	private String type;
	private String data;
	private int cod_soil;
	
	
	public Soil(int id,String t,String dt,int cd) {
		id_soil=id;
		type=t;
		data=dt;
		cod_soil=cd;
	}
	
	public int getSoilID() {
		return id_soil;
		
	}
	public String getType() {
		return type;
	}
		
	
	public String getSensData() {
		return data;
		
	}
	public int getCodSoil() {
		return cod_soil;
		
	}
	
	public setSois
	

}
